	<queryString>
		<![CDATA[SELECT *,
 pl.nama as nama_pelanggan,
 pg.nama as nama_petugas,
 m.nama as nama_menu
FROM pembelian p 
RIGHT JOIN pelanggan pl
ON p.id_pelanggan = pl.id_pelanggan
RIGHT JOIN pegawai pg 
ON p.id_pegawai = pg.id_pegawai
RIGHT JOIN detail_pembelian db
ON p.no_pembelian = db.no_pembelian
RIGHT JOIN menu m
ON db.kode_menu = m.kode_menu
WHERE p.no_pembelian = $P{no}]]>
	</queryString>
